{
    "name": "violinist-dev/timeframe-handler",
    "description": "Timeframe handler for the timeframe config",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eirik S. Morland",
            "email": "eirik@morland.no"
        }
    ],
    "autoload": {
      "psr-4": {
        "Violinist\\TimeFrameHandler\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Violinist\\TimeFrameHandler\\Tests\\": "tests/src"
      }
    },
    "minimum-stability": "dev",
    "scripts": {
      "phpstan": "phpstan analyse src tests",
      "test": "@composer lint && phpunit",
      "lint": "phpcs -p -n"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5||^7||^8",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "require": {
        "violinist-dev/violinist-config": "^1.4"
    }
}
